{
  "name": "ra-hasura-typescript-boilerplate",
  "version": "1.0.0",
  "description": "react admin hasura typescript boilerplate",
  "default": "dist/index.html",
  "sideEffects": false,
  "scripts": {
    "dev": "webpack-dev-server --config ./config/webpack.config.dev.js",
    "clean": "rm -rf dist",
    "build": "npm run clean && npm run lint && webpack --config ./config/webpack.config.prod.js",
    "build:prod": "npm run clean && npm run lint && webpack --env.NODE_ENV=production --config ./config/webpack.config.prod.js",
    "test": "cypress run",
    "cy:open": "cypress open",
    "lint": "eslint --ext .js,.ts src"
  },
  "keywords": [
    "react",
    "admin",
    "hasura",
    "typescript",
    "boilerplate"
  ],
  "author": "Toan Nguyen",
  "license": "MIT",
  "targets": {
    "default": {
      "publicUrl": "./"
    }
  },
  "devDependencies": {
    "@types/node": "^14.6.2",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "cypress": "^5.1.0",
    "cypress-react-unit-test": "^4.11.2",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^2.0.0",
    "eslint": "^7.8.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-functional": "^3.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-react": "^7.20.6",
    "html-webpack-plugin": "^4.4.1",
    "husky": "^4.2.5",
    "source-map-loader": "^1.1.0",
    "ts-loader": "^8.0.3",
    "typescript": "^4.0.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@apollo/client": "^3.1.4",
    "@apollo/link-ws": "^2.0.0-beta.3",
    "@apollo/react-hooks": "^4.0.0",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.5",
    "graphql": "^15.3.0",
    "ra-data-hasura-graphql": "^0.1.12",
    "react": "^16.13.1",
    "react-admin": "^3.8.3",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "subscriptions-transport-ws": "^0.9.18"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run build",
      "pre-push": "npm run build"
    }
  }
}
