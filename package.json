{
  "name": "ra-hasura-typescript-boilerplate",
  "version": "1.0.0",
  "description": "react admin hasura typescript boilerplate",
  "default": "dist/index.html",
  "scripts": {
    "start": "parcel serve ./src/index.html --log-level verbose",
    "clean": "rm -rf .parcel-cache && rm -rf dist",
    "build": "npm run clean && npm run lint && parcel build ./src/index.html --no-cache",
    "build:prod": "npm run clean && NODE_ENV=production parcel build ./src/index.html  --no-cache",
    "test": "cypress run",
    "cy:open": "cypress open",
    "lint": "eslint --ext .js,.ts src"
  },
  "keywords": [
    "react",
    "admin",
    "hasura",
    "typescript",
    "boilerplate"
  ],
  "author": "Toan Nguyen",
  "license": "MIT",
  "targets": {
    "default": {
      "publicUrl": "./"
    }
  },
  "devDependencies": {
    "@parcel/validator-eslint": "^2.0.0-alpha.3",
    "@parcel/validator-typescript": "^2.0.0-alpha.3",
    "@types/node": "^13.13.4",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "cypress": "^4.5.0",
    "cypress-react-unit-test": "^4.1.3",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-functional": "^3.0.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-react": "^7.19.0",
    "husky": "^4.2.5",
    "parcel": "^2.0.0-nightly.248",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@apollo/client": "^3.0.0-beta.44",
    "@apollo/link-ws": "^2.0.0-beta.3",
    "@apollo/react-hooks": "^3.1.5",
    "@material-ui/core": "^4.9.12",
    "@material-ui/icons": "^4.9.1",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.7",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.5",
    "graphql": "^15.0.0",
    "ra-data-hasura-graphql": "^0.1.10",
    "react": "^16.13.1",
    "react-admin": "^3.4.3",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "subscriptions-transport-ws": "^0.9.16"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run build"
    }
  }
}
